+ * distributed under the License is distributed on an "AS IS" BASIS, WITHOUT
+ * WARRANTIES OR CONDITIONS OF ANY KIND, either express or implied.  See the
+ * License for the specific language governing permissions and limitations
+ * under the License.
+ *
+ ****************************************************************************/
+
+/****************************************************************************
+ * Included Files
+ ****************************************************************************/
+
+#include <nuttx/config.h>
+
+#include <debug.h>
+
+#include <nuttx/kmalloc.h>
+#include <netinet/if_ether.h>
+#include <nuttx/net/netdev_lowerhalf.h>
+
+#include <chip.h>
+
+#include "sksoc_openeth.h"
+/****************************************************************************
+ * Pre-processor Definitions
+ ****************************************************************************/
+
+#define ETHERNET_MTU 1500
+#define REGETH_TXSIZE_W       (ETHERNET_ADDR + ETHERNET_MTU+0)
+#define REGETH_RXREAD_W       (ETHERNET_ADDR + ETHERNET_MTU+4)
+#define REGETH_SENDPACKET_W   (ETHERNET_ADDR + ETHERNET_MTU+8)
+#define REGETH_TXENABLEIRQ_W  (ETHERNET_ADDR + ETHERNET_MTU+24)
+#define REGETH_RXENABLEIRQ_W  (ETHERNET_ADDR + ETHERNET_MTU+28)
+#define REGETH_ACKTXIRQ_W     (ETHERNET_ADDR + ETHERNET_MTU+32)
+#define REGETH_ADDTOPACKET_W  (ETHERNET_ADDR)
+#define REGETH_TXSTATE_R      (ETHERNET_ADDR + ETHERNET_MTU+12)
+#define REGETH_RXSIZE_R       (ETHERNET_ADDR + ETHERNET_MTU+16)
+#define REGETH_RXWRITE_R      (ETHERNET_ADDR + ETHERNET_MTU+36)
+#define REGETH_RDFROMPACKET_R (ETHERNET_ADDR)
+
+#define REG_READ(_r)         (*(volatile uint32_t *)(_r))
+#define REG_WRITE(_r, _v)    (*(volatile uint32_t *)(_r)) = (_v)
+
+/****************************************************************************
+ * Private Types
+ ****************************************************************************/
+
+struct openeth_priv_s
+{
+  struct netdev_lowerhalf_s dev;
+};
+
+/****************************************************************************
+ * Private Function Prototypes
+ ****************************************************************************/
+
+/* NuttX callback functions */
+
+static int openeth_ifup(struct netdev_lowerhalf_s *dev);
+static int openeth_ifdown(struct netdev_lowerhalf_s *dev);
+static int openeth_transmit(struct netdev_lowerhalf_s *dev, netpkt_t *pkt);
+static netpkt_t *openeth_receive(struct netdev_lowerhalf_s *dev);
+
+/****************************************************************************
+ * Private Data
+ ****************************************************************************/
+
+static struct openeth_priv_s g_openeth;
+
+static const struct netdev_ops_s g_ops =
+{
+  .ifup     = openeth_ifup,
+  .ifdown   = openeth_ifdown,
+  .transmit = openeth_transmit,
+  .receive  = openeth_receive,
+};
+
+/****************************************************************************
+ * Private Functions
+ ****************************************************************************/
+
+static inline void openeth_reset(void)
+{
+}
+
+static int rx_isr_handler(int irq, void *context, void *arg)
+{
+  struct netdev_lowerhalf_s *dev = &g_openeth.dev;
+  unsigned int aux, rxread, rxwrote;
+
+  // disable rx interrupts 
+  REG_WRITE(REGETH_RXENABLEIRQ_W, 0);
+  
+  aux = REG_READ(REGETH_RXWRITE_R);
+  rxread = aux >> 16;
+  rxwrote = aux & 0xffff;
+  //ninfo("rxinterrupt: rxread=%d rxwrote=%d\n", rxread, rxwrote);
+  if((rxwrote-rxread) == 1) {
+    netdev_lower_rxready(dev);
+  } else {
+    return -1;
+  }
+
+  return 0;
+}
+
+static uint8_t rxdevbuf[1600], txdevbuf[1600];
+static netpkt_t *openeth_receive(struct netdev_lowerhalf_s *dev)
+{
+  netpkt_t *pkt = netpkt_alloc(dev, NETPKT_RX);
+  int i, len;
+  unsigned int aux;
+
+  //ninfo("openeth_receive\n");
+  if (pkt)
+    {
+      len = REG_READ(REGETH_RXSIZE_R);
+      ninfo("rxlen[%d]", len);
+      for(i = 0; i < len; i += 4) {
+        aux = REG_READ(REGETH_RDFROMPACKET_R + i);
+	rxdevbuf[i+0] = (aux >> 0) & 0xff;
+	rxdevbuf[i+1] = (aux >> 8) & 0xff;
+	rxdevbuf[i+2] = (aux >> 16) & 0xff;
+	rxdevbuf[i+3] = (aux >> 24) & 0xff;
+	//ninfo("rx[%d]: %x %x %x %x", i, rxdevbuf[i], rxdevbuf[i+1], rxdevbuf[i+2], rxdevbuf[i+3]);
+      }
+      netpkt_copyin(dev, pkt, rxdevbuf, len, 0);
+    } else 
+      goto err;
+
+  // tell controller that we have read the packet, and clear the interrupt
+  do {
+    REG_WRITE(REGETH_RXREAD_W, 1);
+    aux = REG_READ(REGETH_RXWRITE_R);
+  } while ((aux & 0xffff) != (aux >> 16));
+  // enable rx interrupts
+  REG_WRITE(REGETH_RXENABLEIRQ_W, 1);
+
+  return pkt;
+
+err:
+  nerr("openeth_receive error\n");
+  netpkt_free(dev, pkt, NETPKT_RX);
+  return NULL;
+}
+
+/****************************************************************************
+ * Name: openeth_ifup
+ *
+ * Description:
+ *   NuttX Callback: Bring up the Ethernet interface when an IP address is
+ *   provided
+ *
+ * Input Parameters:
+ *   dev - Reference to the NuttX driver state structure
+ *
+ * Returned Value:
+ *   None
+ *
+ * Assumptions:
+ *
+ ****************************************************************************/
+
+static int openeth_ifup(struct netdev_lowerhalf_s *dev)
+{
+  irqstate_t flags;
+
+  netdev_lower_carrier_on(dev);
+
+  /* Disable the Ethernet interrupt */
+
+  flags = enter_critical_section();
+
+  //dev->d_flags |= IFF_UP;
+
+  /* Enable TX and RX interrupts */
+  ninfo("ifup enable irqs\n");
+  REG_WRITE(REGETH_TXENABLEIRQ_W, 1);
+  REG_WRITE(REGETH_RXENABLEIRQ_W, 1);
+
+  leave_critical_section(flags);
+
+  return OK;
+}
+
+/****************************************************************************
+ * Name: openeth_ifdown
+ *
+ * Description:
+ *   NuttX Callback: Stop the interface.
+ *
+ * Input Parameters:
+ *   dev - Reference to the NuttX driver state structure
+ *
+ * Returned Value:
+ *   None
+ *
+ * Assumptions:
+ *
+ ****************************************************************************/
+
+static int openeth_ifdown(struct netdev_lowerhalf_s *dev)
+{
+  irqstate_t flags;
+
+  /* Disable the Ethernet interrupt */
+
+  flags = enter_critical_section();
+
+  /* Disable TX and RX interrupts*/
+  REG_WRITE(REGETH_TXENABLEIRQ_W, 0);
+  REG_WRITE(REGETH_RXENABLEIRQ_W, 0);
+
+  leave_critical_section(flags);
+
+  netdev_lower_carrier_off(dev);
+
+  return OK;
+}
+
+//--------------------------------------------------------------------------
+
+static int tx_isr_handler(int irq, void *context, void *arg)
+{
+  struct netdev_lowerhalf_s *dev = &g_openeth.dev;
+
+  REG_WRITE(REGETH_ACKTXIRQ_W, 1);
+  REG_WRITE(REGETH_ACKTXIRQ_W, 0);
+  netdev_lower_txdone(dev);
+  return 0;
+}
+
+static int openeth_transmit(struct netdev_lowerhalf_s *dev,
+                            netpkt_t *pkt)
+{
+  unsigned int len = netpkt_getdatalen(dev, pkt);
+  int i, j, aux;
+
+  if(dev != (struct netdev_lowerhalf_s *)&g_openeth) {
+    nerr("dev != &g_openeth");
+    return -1;
+  }
+
+  ninfo("transmit len=%d\n", len);
+  if(len > ETHERNET_MTU) {
+    nerr("transmit len=%d > ETHERNET_MTU\n", len);
+    return -1;
+  }
+  /* Copyout the L2 data and transmit. */
+
+  netpkt_copyout(dev, txdevbuf, pkt, len, 0);
+
+  REG_WRITE(REGETH_TXSIZE_W, len);
+  /* Do Transmit */
+  for(i = 0; i < len; i += 4) {
+    aux = 0;
+    for(j = 3; j >= 0; j--) {
+      //ninfo("tx[%d]=%x \n", i+3-j, txdevbuf[i+3-j]);
+      aux = (aux << 8) | txdevbuf[i+j];
+    }
+    REG_WRITE(REGETH_ADDTOPACKET_W + i, aux);
+  }
+  REG_WRITE(REGETH_SENDPACKET_W, 1);
+
+  /* Free the buffer and notify the upper layer */
+
+  netpkt_free(dev, pkt, NETPKT_TX);
+
+  return OK;
+}
+
+static int openeth_set_addr(uint8_t *addr)
+{
+  ninfo("set mac\n");
+  if (!addr)
+    {
+      nerr("can't set mac addr to null");
+      goto err;
+    }
+
+  const uint8_t mac0[4] = {
+    addr[5], addr[4], addr[3], addr[2]
+  };
+
+  const uint8_t mac1[4] = {
+    addr[1], addr[0]
+  };
+
+  uint32_t mac0_u32;
+  uint32_t mac1_u32;
+  memcpy(&mac0_u32, &mac0, 4);
+  memcpy(&mac1_u32, &mac1, 4);
+  //REG_WRITE(OPENETH_MAC_ADDR0_REG, mac0_u32);
+  //REG_WRITE(OPENETH_MAC_ADDR1_REG, mac1_u32);
+  return 0;
+err:
+  return -1;
+}
+
+/****************************************************************************
+ * Public Functions
+ ****************************************************************************/
+
+/****************************************************************************
+ * Name: sksoc_openeth_initialize
+ *
+ * Description:
+ *   Initialize the openeth driver
+ *
+ * Input Parameters:
+ *   None
+ *
+ * Returned Value:
+ *   OK on success; Negated errno on failure.
+ *
+ ****************************************************************************/
+
+int sksoc_openeth_initialize(void)
+{
+  int ret, i;
+  struct openeth_priv_s *priv = &g_openeth;
+  struct netdev_lowerhalf_s *dev = &priv->dev;
+
+  /* Initialize the driver structure */
+
+  memset(priv, 0, sizeof(struct openeth_priv_s));
+
+  dev->ops = &g_ops;
+
+  dev->quota[NETPKT_TX] = 1;
+  dev->quota[NETPKT_RX] = 1;
+
+  /* Allocate DMA buffers */
+
+  /* Setup interrupts */
+
+  /* Initialize the MAC */
+
+  openeth_reset();
+  memcpy(priv->dev.netdev.d_mac.ether.ether_addr_octet,
+    "\x00\x02\x03\x04\x05\x06\x07\x08", ETH_ALEN);
+  openeth_set_addr(priv->dev.netdev.d_mac.ether.ether_addr_octet);
+  for(i = 0; i < ETH_ALEN; i++)
+    ninfo("mac addr: [%d]=%x \n", i, priv->dev.netdev.d_mac.ether.ether_addr_octet[i]);
+
+  /* Attach the interrupts */
+  ret = irq_attach(RISCV_IRQ_EXT+1, tx_isr_handler, NULL);
+  ret = irq_attach(RISCV_IRQ_EXT+2, rx_isr_handler, NULL);
+
+  /* Register the device with the OS so that socket IOCTLs can be
+   * performed.
+   */
+
+  ret = netdev_lower_register(dev, NET_LL_ETHERNET);
+  if (ret)
+    {
+      nerr("ERROR: netdev_lower_register\n");
+      goto err;
+    }
+
+  /* Put the network in the UP state */
+
+  return openeth_ifup(dev);
+
+err:
+  nerr("Failed initializing ret = %d", ret);
+  abort();
+}
+
